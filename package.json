{
  "name": "mix-quick",
  "version": "0.0.1",
  "description": "Empty project",
  "main": "index.html",
  "scripts": {
    "dev": "npm run development",
    "prod": "npm run production",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "clean": "rimraf public/* public/{,**/}*{html,json}",
    "check": "prettier --check resources/**/*.{css,js,scss,vue}",
    "format": "prettier --write resources/**/*.{css,js,scss,vue}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.10",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "^3.7.0",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.3.0",
    "laravel-mix": "^5.0.4",
    "laravel-mix-copy-watched": "^2.2.4",
    "laravel-mix-dump": "^1.0.1",
    "laravel-mix-eslint": "^0.1.3",
    "laravel-mix-twig-to-html": "^1.3.0",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "twig-html-loader": "^0.1.8",
    "vue-eslint-parser": "^7.1.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "browserslist": {
    "development": "last 1 version",
    "production": ">2%, Firefox ESR"
  },
  "eslintConfig": {
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "warn"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "printWidth": 400,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent"
  }
}
